{
  "name": "serverless-openapi-joi",
  "description": "Serverless plugin for creating OpenAPI specifications with Joi validation",
  "author": "Viljami Kuosmanen <viljami@avoinsorsa.fi>",
  "version": "0.2.5",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "keywords": [
    "serverless",
    "openapi",
    "swagger",
    "joi"
  ],
  "dependencies": {
    "boom": "^7.2.0",
    "joi": "^14.0.0",
    "joi-to-json-schema": "^3.4.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.13",
    "@types/boom": "^7.2.0",
    "@types/jest": "^23.3.5",
    "@types/joi": "^13.6.1",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "jest": "^23.6.0",
    "markdown-toc": "^1.2.0",
    "openapi-schema-validation": "^0.4.2",
    "prettier": "^1.14.3",
    "serverless": "^1.32.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "test": "NODE_ENV=test jest -i"
  }
}
